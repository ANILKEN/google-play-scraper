{
    "name": "anilken/google-play-scraper",
    "type": "library",
    "description": "A PHP scraper to get app data from Google Play",
    "keywords": ["google play", "scraper"],
    "homepage": "https://github.com/anilken/google-play-scraper",
    "license": "MIT",
    "require": {
        "fabpot/goutte": "^3.0"
    },
    "authors": [
        {
            "name": "Raul Rodriguez",
            "email": "raul@raulr.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anilken\\GooglePlayScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anilken\\GooglePlayScraper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "mockery/mockery": "^1.0"
    }
}
